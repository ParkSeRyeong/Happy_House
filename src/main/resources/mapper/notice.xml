<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.happyhouse.model.mapper.NoticeMapper">

	
	<select id="noticeList" resultType="com.ssafy.happyhouse.model.dto.NoticeDto">
		select no,title,views,date,content
		from notices
	</select>
	
	<select id="noticeView" parameterType="String" resultType="com.ssafy.happyhouse.model.dto.NoticeDto">
		select no,title,views,date,content
		from notices
		where no = #{no}
	</select> 
	
