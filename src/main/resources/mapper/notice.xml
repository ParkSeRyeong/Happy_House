<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssafy.happyhouse.model.mapper.NoticeMapper">

	
	<select id="noticeList" resultType="NoticeDto">
		select no,title,views,date,content
		from notices
	</select>
	
	<select id="noticeView" parameterType="NoticeDto" resultType="Boolean">
		select no,title,views,date,content
		from notices
		where no = #{no}
	</select> 
	
	<insert id="noticeWrite" parameterType="NoticeDto">
		insert into notices (title, content)
		values (#{title}, #{content})
	</insert> 
	
